<div id ="random-beer">
    {{>cardBeerPartial this}}
    <div id="info-container" >

        <p><b>Food Pairing:</b></p>
    
        {{#each this.food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    
    
        <p><b>Brewers tips:</b>
        {{this.brewers_tips}}</p>
    
    </div>
</div>